  <section>
      <div class=container>
          <div class="row">
              <a href="/admin/add-product" class="btm btn-success ml-auto mt-4">Add product</a>
        </div>
        
      <table class="table mt-3">
        <h2>Your Products</h2>
    <thead>
      <tr>
        <th scope="col">No</th>
        <th scope="col">Title</th>
        <th scope="col">Category</th>
        <th scope="col">Description</th>
        <th scope="col">Image</th>
      </tr>
    </thead>
    
    <tbody>
      {{#each products}}
      <tr>
        <th scope="row">1</th>
        <td>{{this.Name}}</td>
        <td>{{this.Category}}</td>
        <td>{{this.Description}}</td>
        <td><img style="width:60px;height:60px" src="/images/{{this._id}}.jpg"></td>
         <td><a  class="btn btn-success" href='/updateProduct?proId={{this._id}}&name=jerin'>Update</a></td>
         <td><a  class="btn btn-primary" href='/merchantViewProduct?proId={{this._id}}'>View product</a></td>
        <td><a class="btn btn-danger" href='/removeProduct?id={{this._id}}&name=jerin'>Remove</a></td>
        
      </tr>
      {{/each}}
  
        </div>
    </tbody>
  </table>

  </section>